{
    "name": "languara/fuelphp",
    "keywords": ["Languara", "FuelPHP", "Translation", "Internationalization"],
    "homepage": "http://languara.com",
    "description": "Languara integration plugin for FuelPHP 1.x. Easily synchronize your content with Languara.",
    "type" : "fuel-package",
    "target-dir": "languara",
    "license": "MIT",
    "authors": [
        {
            "name": "Languara Development Team",
            "email": "support@languara.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "languara/plugin-core": "1.0.*@dev"
    },
    "autoload":    {
        "psr-4": {
            "Languara\\": "",
            "Fuel\\Tasks\\": "tasks/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
